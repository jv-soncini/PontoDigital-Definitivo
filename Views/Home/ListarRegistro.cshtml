@model PontoDigital_Definitivo.ViewModel.ComentariosViewModel
<table>
     <thead  >
         <tr>
             <th>Id</th>
             <th>Nome</th>
             <th>Email</th>
             <th>Telefone</th>
             <th>Data de Nascimento</th>
         </tr>
     </thead>
 <tbody>
         @foreach (var item in Model.Clientes)
         {
     <tr>
             @if(item == null){
                 continue;
             }
         <td>@item.Id</td>
         <td>@item.Nome</td>
         <td>@item.Email</td>
         <td>@item.Telefone</td>
         <td>@item.DataNascimento.ToShortDateString()</td>
     </tr>
         }
 </tbody>
 </table>